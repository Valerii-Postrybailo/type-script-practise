{
  "env": {
    "browser": true,
    "es6": true,
    "node":true
  },

  "extends":["airbnb/base"],

  "rules": { 
    "semi": ["error", "always"],
    "no-plusplus": "off",
    "eol-last":"off",
    "max-len":["error", 150],
    "quotes": "warn",
    "no-tabs": "off",
    "no-alert": "off",
    "indent": [2, "tab"]
  }
}